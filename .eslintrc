{
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "node": true,
    "shared-node-browser": true,
    "mocha": true
  },
  "plugins": [
    "security"
  ],
  "ecmaFeatures": {
    "arrowFunctions": true,
    "classes": true,
    "defaultParams": true,
    "destructuring": true,
    "forOf": true,
    "generators": false,
    "modules": true,
    "objectLiteralComputedProperties": true,
    "objectLiteralShorthandMethods": true,
    "objectLiteralShorthandProperties": true,
    "spread": true,
    "templateStrings": true
  },
  "rules": {
    "comma-dangle": [1,"always-multiline"],
    "no-cond-assign": [2,"always"],
    "no-constant-condition": 2,
    "no-control-regex": 2,
    "no-debugger": 2,
    "no-dupe-args": 2,
    "no-dupe-keys": 2,
    "no-duplicate-case": 2,
    "no-empty-character-class": 2,
    "no-empty": 2,
    "no-ex-assign": 2,
    "no-extra-boolean-cast": 2,
    "no-extra-parens": [2,"all"],
    "no-extra-semi": 2,
    "no-func-assign": 2,
    "no-inner-declarations": [2,"functions"],
    "no-invalid-regexp": 2,
    "no-irregular-whitespace": 2,
    "no-negated-in-lhs": 2,
    "no-obj-calls": 2,
    "no-regex-spaces": 2,
    "no-sparse-arrays": 2,
    "no-unreachable": 2,
    "use-isnan": 2,
    "valid-typeof": 2,
    "no-unexpected-multiline": 2,
    "block-scoped-var": 2,
    "complexity": [2,15],
    "curly": [2,"all"],
    "default-case": 2,
    "dot-notation": [2,{"allowKeywords":true}],
    "dot-location": [2,"property"],
    "eqeqeq": [2,"smart"],
    "guard-for-in": 2,
    "no-alert": 2,
    "no-caller": 2,
    "no-else-return": 2,
    "no-labels": 2,
    "no-eval": 2,
    "no-extend-native": 2,
    "no-extra-bind": 2,
    "no-fallthrough": 2,
    "no-floating-decimal": 1,
    "no-implicit-coercion": [2,{"boolean":false,"string":true}],
    "no-implied-eval": 2,
    "no-invalid-this": 1,
    "no-iterator": 2,
    "no-lone-blocks": 2,
    "no-loop-func": 1,
    "no-multi-spaces": 2,
    "no-native-reassign": 2,
    "no-new-func": 2,
    "no-new-wrappers": 2,
    "no-new": 2,
    "no-octal-escape": 2,
    "no-octal": 2,
    "no-proto": 2,
    "no-redeclare": 2,
    "no-return-assign": [2,"always"],
    "no-self-compare": 2,
    "no-sequences": 2,
    "no-throw-literal": 2,
    "no-unused-expressions": 2,
    "no-useless-call": 2,
    "no-void": 2,
    "no-with": 2,
    "radix": 2,
    "vars-on-top": 1,
    "wrap-iife": [2,"outside"],
    "yoda": [2,"never",{"exceptRange":true}],
    "no-catch-shadow": 2,
    "no-delete-var": 1,
    "no-label-var": 2,
    "no-shadow-restricted-names": 2,
    "no-shadow": 2,
    "no-undef-init": 2,
    "no-undef": 2,
    "no-unused-vars": 1,
    "no-use-before-define": 2,
    "callback-return": [2,["\"callback\""," \"cb\""," \"next\""," \"done\""," \"resolve\""," \"reject\""," \"res\""," \"response\""," \"req\""," \"request\""]],
    "handle-callback-err": [2,"\"^(err|error)$\""],
    "no-new-require": 2,
    "no-sync": 1,
    "array-bracket-spacing": [1,"always",{"singleValue":false,"objectsInArrays":true,"arraysInArrays":true}],
    "brace-style": [1,"stroustrup",{}],
    "camelcase": [1,{"properties":"never"}],
    "comma-spacing": [1,{"after":true}],
    "comma-style": [1,"last"],
    "computed-property-spacing": [1,"never"],
    "consistent-this": [1,"\"self\""],
    "eol-last": 2,
    "func-style": [1,"declaration"],
    "indent": [1,2],
    "key-spacing": [1,{"afterColon":true}],
    "lines-around-comment": [1,{"beforeBlockComment":true,"beforeLineComment":true,"allowBlockStart":true}],
    "linebreak-style": [1,"unix"],
    "max-nested-callbacks": [1,3],
    "new-cap": [1,{"newIsCap":true}],
    "new-parens": 1,
    "newline-after-var": [1,"always"],
    "no-array-constructor": 2,
    "no-inline-comments": 1,
    "no-lonely-if": 2,
    "no-mixed-spaces-and-tabs": 2,
    "no-multiple-empty-lines": [1,{"max":2}],
    "no-new-object": 2,
    "no-spaced-func": 2,
    "no-trailing-spaces": 1,
    "no-underscore-dangle": 1,
    "no-unneeded-ternary": 2,
    "object-curly-spacing": [1,"always",{}],
    "operator-assignment": [1,"always"],
    "operator-linebreak": [1,"after"],
    "padded-blocks": [1,"never"],
    "quotes": [1,"single","avoid-escape"],
    "semi-spacing": [1,{"after":true}],
    "semi": [2,"always"],
    "space-before-blocks": [1,"always"],
    "space-before-function-paren": [1,"never"],
    "space-in-parens": [1,"never"],
    "space-infix-ops": 1,
    "keyword-spacing": [1,{"before":true,"after":false,"overrides": {"else": { "after": true }, "return": { "after": true }, "try": { "after": true }}}],
    "space-unary-ops": [1,{"words":true}],
    "spaced-comment": [1,"always",{"exceptions":["\"-\""," \"+\""," \"=\""," \"*\""]}],
    "arrow-parens": [1,"as-needed"],
    "arrow-spacing": [1,{"before":true,"after":true}],
    "arrow-body-style": [1, "as-needed"],
    "constructor-super": 1,
    "generator-star-spacing": [1,"before"],
    "no-class-assign": 2,
    "no-const-assign": 2,
    "no-this-before-super": 2,
    "no-var": 1,
    "object-shorthand": [1,"always"],
    "prefer-const": 1,
    "prefer-spread": 1,
    "require-yield": 2,
    "max-depth": [1,3],
    "max-len": [1,{"code":120,"tabWidth":2,"ignoreRegExpLiterals":true,"ignoreComments":true,"ignoreUrls":true,"ignoreTemplateLiterals":true}],
    "security/detect-unsafe-regex": 2,
    "security/detect-buffer-noassert": 2,
    "security/detect-child-process": 2,
    "security/detect-disable-mustache-escape": 2,
    "security/detect-eval-with-expression": 2,
    "security/detect-no-csrf-before-method-override": 2,
    "security/detect-non-literal-fs-filename": 1,
    "security/detect-non-literal-regexp": 2,
    "security/detect-non-literal-require": 1,
    "security/detect-object-injection": 1,
    "security/detect-possible-timing-attacks": 2,
    "security/detect-pseudoRandomBytes": 2
  }
}
