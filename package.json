{
  "name": "@belsrc/squeue",
  "version": "0.3.1",
  "description": "Small simple ip blocker for connect/express",
  "main": "index.js",
  "scripts": {
    "cov-test": "node node_modules/.bin/istanbul cover --dir=documents/coverage --print=detail node_modules/.bin/_mocha -- -R spec --ui tdd --recursive tests",
    "test": "node node_modules/.bin/mocha --reporter spec --ui tdd --recursive tests",
    "lint": "node node_modules/.bin/eslint -c .eslintrc --color --quiet --global __base --ignore-path .eslintignore \"lib/**\"",
    "analyze": "node node_modules/.bin/es6-plato -r -l .eslintrc -d documents/analysis lib",
    "docs": "node node_modules/.bin/jsdoc -r lib --readme README.md --destination documents/documentation --package package.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/belsrc/express-ip-block.git"
  },
  "keywords": [
    "ip",
    "ip filter",
    "ip-filter",
    "ip block",
    "whitelist",
    "blacklist",
    "express",
    "connect"
  ],
  "author": "belsrc",
  "license": "DBAD",
  "bugs": {
    "url": "https://github.com/belsrc/express-ip-block/issues"
  },
  "homepage": "https://github.com/belsrc/express-ip-block#readme",
  "dependencies": {},
  "devDependencies": {
    "chai": "3.5.0",
    "chai-as-promised": "6.0.0",
    "cross-env": "4.0.0",
    "es6-plato": "1.0.14",
    "eslint": "3.19.0",
    "eslint-plugin-security": "1.3.0",
    "istanbul": "0.4.5",
    "jsdoc": "3.4.3",
    "mocha": "3.2.0"
  }
}
